<script>
$(document).on('click', '.roomJoinBtn', function() {
    var room = $(this).data('id');
	var password = '';
	if (chatHTML5.rooms[room].isPasswordProtected==='1') {
		bootbox.prompt("Enter password", function(password) {                
			if (!password) {
				return;
			}
			$.post('/ajax.php', {a:'checkPasswordRoom', id:room, password:password}, function(res) {
				if (res==='ok') {
					chatHTML5.changeRoom(room);
					return;
				}
				bootbox.alert('Incorrect password');
			});
		});
		return;
	}
	chatHTML5.closeAllTabs();
	chatHTML5.changeRoom(room);
});
</script>    <!-- Modal -->
